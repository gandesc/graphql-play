type Query {
    products: [Product]
}

interface Product {
    id: ID,
    description: String,
    price: Int
}

type Fruit implements Product {
    id: ID,
    description: String,
    price: Int
    expiration: String
}

type Book implements Product {
    id: ID,
    description: String,
    price: Int
    author: String
}

type Electronics implements Product {
    id: ID,
    description: String,
    price: Int
    brand: Brand
}

enum Brand {
    APPLE,
    SAMSUNG
}

